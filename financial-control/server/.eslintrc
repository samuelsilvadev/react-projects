{
	"extends": "airbnb-base",
	"rules": {
		"import/newline-after-import": 0,
		"no-tabs": 0,
		"translate_tabs_to_spaces": true,
		"indent": [2, "tab"],
		"no-console": 0,
		"prefer-promise-reject-errors": 0,
		"no-underscore-dangle": 0,
		"consistent-return": 0,
		"no-shadow": 0,
		"no-plusplus": 0
	}
}
